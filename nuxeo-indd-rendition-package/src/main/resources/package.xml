<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="nuxeo-indd-rendition" version="@VERSION@">
  <title>Nuxeo Adobe InDesign Preview</title>
  <description>
    <p>This plugin provides a preview feature for Adobe InDesign files in the Nuxeo Platform.
      It relies on the jpeg preview embedded in the XMP metadata by InDesign</p>
  </description>
  <home-page>https://github.com/nuxeo-sandbox/nuxeo-indd-rendition</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <!-- Nuxeo Validation: none | inprocess | primary_validation | nuxeo_certified -->
  <nuxeo-validation>none</nuxeo-validation>
  <!-- Production State: production_ready | testing | proto -->
  <production-state>testing</production-state>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
    <platform>server-@DISTRIB_VERSION@-HF*</platform>
  </platforms>
  <!-- Dependencies on other packages -->
  <dependencies>
    <package>nuxeo-10.10-HF54:1.0.0</package>
  </dependencies>
